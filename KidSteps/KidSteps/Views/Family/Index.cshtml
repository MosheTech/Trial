@model IEnumerable<KidSteps.Models.Family>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @Html.ActionLink("Details", MVC.Family.Details().WithId(item))
            </td>
        </tr>
    }
</table>
